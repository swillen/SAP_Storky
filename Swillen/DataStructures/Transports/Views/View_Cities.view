{
  "viewName":"VIEW_CITIES",
  "query":"SELECT TR.TRANSPORT_TYPE, TA.AIRPORT_CITY, TA.AIRPORT_LATITUDE,TA.AIRPORT_LONGITUDE, TA.AIRPORT_COUNTRY, TA.AIRPORT_TIMEZONE FROM APP.TRANSPORTS  AS tr JOIN APP.TRAVEL_AIRPORTS AS ta ON tr.DEPARTURE_CITY_ID = ta.AIRPORT_ID GROUP BY ta.AIRPORT_CITY, tr.TRANSPORT_TYPE, TA.AIRPORT_LATITUDE,TA.AIRPORT_LONGITUDE,TA.AIRPORT_COUNTRY, TA.AIRPORT_TIMEZONE"
}
